<%- include("header") -%>
    <div class="box" id="heading">
        <h1><%= ListTitle %></h1>
    </div>
    
    <div class="box">
        <%for(let i=0; i<itemlist.length; i++){%>
            <div class = "item">
                <input type="checkbox">
                <p><%= itemlist[i] %> </p>
            </div>
        <%}%>
        <form class = "item" action="/" method="post">
            <input type = "text" name="listItem">
            <button type = "submit" name = "list" value = <%= ListTitle%> + </button>
        </form>

    </div>
<%- include("footer") -%>
